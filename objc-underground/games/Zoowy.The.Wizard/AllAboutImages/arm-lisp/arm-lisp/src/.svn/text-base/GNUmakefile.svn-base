#
# An example GNUmakefile
#

# Include the common variables defined by the Makefile Package
include $(GNUSTEP_MAKEFILES)/common.make

# Two libraries
LIBRARY_NAME = lib-arm-lisp

#
# The files for the lib-arm-lisp library
#
# The Objective-C files to compile
lib-arm-lisp_OBJC_FILES = \
6502AssemblerMachine.m ArmAssemblerMachine.m ArmCompilerBase.m ArmCompiler.m ArmLispCompiler.m ArmLispScanner.m ArmScanner.m ArmSubCompiler.m ArmSubScanner.m ByteCodeMachine.m ClassLocator.m Compiler.m DataArray.m Data.m Dispatcher.m Error.m ErrorSubCompiler.m FileBuffer.m FileName.m FileStream.m FNString.m Form.m Fuzzy.m GBAObject.m HashTable.m HashTableStrings.m HashTableTupleInts.m Hex.m JavaByteCodeMachine.m MethodNameArray.m MethodName.m Node.m ObjectLocator.m Object.m Operator.m OperatorTable.m ParseString.m ParseTree.m ParseTreeOpLeaf.m ParseTreeOp.m Scanner.m ScanSystem.m Stack.m Status.m StructureBracket.m Structure.m SymbolArm.m Symbol.m SymbolTableArm.m SymbolTable.m TupleInt.m TupleIterator.m Tuple.m TypeChar.m TypeInt.m Type.m TypeNameArray.m TypeName.m TypeNameMem.m TypeTable.m TypeVoid.m Util.m WordBase.m Word.m \

# The C source files to be compiled
lib-arm-lisp_C_FILES =

# The PostScript wrap source files to be compiled
##FIXMElib-arm-lisp_PSWRAP_FILES = drawing.psw

# The header files for the library
lib-arm-lisp_HEADER_FILES_DIR = ###FIXME./lib-arm-lisp
lib-arm-lisp_HEADER_FILES_INSTALL_DIR = lib-arm-lisp 
lib-arm-lisp_HEADER_FILES = arm-lisp-config.h 

#
# The files for the libtwo library
#
# The Objective-C files to compile
###libtwo_OBJC_FILES = two.m another.m test.m

# The header files for the library
###libtwo_HEADER_FILES_DIR = ./two
###libtwo_HEADER_FILES_INSTALL_DIR = two
###libtwo_HEADER_FILES = two.h another.h test.h common.h

# Option include to set any additional variables
-include GNUmakefile.preamble

# Include in the rules for making libraries
include $(GNUSTEP_MAKEFILES)/library.make

# Option include to define any additional rules
#include GNUmakefile.postamble
#
# GNUmakefile - Generated by ProjectCenter
#
ifeq ($(GNUSTEP_MAKEFILES),)
 GNUSTEP_MAKEFILES := $(shell gnustep-config --variable=GNUSTEP_MAKEFILES 2>/dev/null)
endif
ifeq ($(GNUSTEP_MAKEFILES),)
 $(error You need to set GNUSTEP_MAKEFILES before compiling!)
endif

include $(GNUSTEP_MAKEFILES)/common.make

#
# Application
#
VERSION = 0.2
PACKAGE_NAME = arm-lisp
APP_NAME = arm-lisp
arm-lisp_APPLICATION_ICON = arm-lisp.tiff


#
# Resource files
#
arm-lisp_RESOURCE_FILES = \
Resources/arm-lisp.tiff \

#
# Header files
#
arm-lisp_HEADER_FILES = \

#
# Class files
#
arm-lisp_OBJC_FILES = \
main.m

#
# Other sources
#
arm-lisp_OBJC_FILES += \

arm-lisp_OBJC_LIBS += \
obj/lib-arm-lisp.so
#
# Makefiles
#
-include GNUmakefile.preamble
include $(GNUSTEP_MAKEFILES)/aggregate.make
include $(GNUSTEP_MAKEFILES)/application.make
-include GNUmakefile.postamble
